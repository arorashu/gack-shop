doctype html
html
  head
    block meta
      title= "Innovision, NSIT"
    meta(name="theme-color" content="#ffffff")
    meta(name='viewport', content='width=device-width, initial-scale=1.0')
    link(rel='stylesheet', href='/font/material\ icons/material-icons.css')
    link(href='/stylesheets/materialize.min.css', rel='stylesheet', media='screen')
    link(rel='stylesheet', href='/stylesheets/style.css')
    link(rel='stylesheet', href='/font/intro/intro.css')
    link(rel='manifest' href='/manifest.json')
    link(rel='stylesheet', href='https://fonts.googleapis.com/css?family=Open+Sans:400,800,300', type='text/css')
    style.
      @media only screen and (max-width: 600px) {
        #address {
          text-align: center !important;
        }
      }
      #listicon {
        margin-right:0;
      }
      h1, h2, h3, h4, h5, h6, p, li, i, div, .btn, a {
        color: #fff !important;
      }
      #big {
        display: none;
      }
    block style
    if !is_admin
      script.
        (function (i, s, o, g, r, a, m) {
          i['GoogleAnalyticsObject'] = r;
          i[r] = i[r] || function () {
